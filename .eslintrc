{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "jest": true
  },
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "no-use-before-define": 0,
    "no-param-reassign": "off",
    "arrow-parens": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "react/prefer-stateless-function": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "aspects": ["invalidHref"]
      }
    ]
  }
}
